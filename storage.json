{
	"IterationLimit": 20,
	"LogLevel": "Debug",
	"LogPath": "/var/log/tires/tires.log",
	"TemporaryPath": "tmp",
	"RunInterval": "hourly",
	"ProcessPriority": 2,
	"Tiers": [
		{
			"target": 90,
			"path": "/mnt/hot"
		},
		{
			"target": 100,
			"path": "/mnt/cold"
		}
	],
	"FolderRules": [
		{
			"PathPrefix": "videos",
			"Priority": 10,
			"RuleType": "Size",
			"Reverse": true
		},
		{
			"PathPrefix": "documents",
			"Priority": 5,
			"RuleType": "Time",
			"TimeType": "Modify"
		},
		{
			"PathPrefix": "archives",
			"Priority": 3,
			"RuleType": "Name",
			"Pattern": ".zip"
		}
	]
}